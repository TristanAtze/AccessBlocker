{
    "version": "0.2.0",
    "configurations": [
        {
            // IntelliSense verwenden, um herauszufinden, welche Attribute für das C#-Debuggen vorhanden sind
            // Hover verwenden, um die Beschreibung der vorhandenen Attribute anzuzeigen
            // Weitere Informationen finden Sie unter https://github.com/dotnet/vscode-csharp/blob/main/debugger-launchjson.md
            "name": ".NET Launch (console) - net10.0",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // Wenn Sie die Zielframeworks geändert haben, stellen Sie sicher, dass Sie den Programmpfad aktualisieren.
            "program": "${workspaceFolder}/AccessBlockerConsolePrototype/bin/Debug/net10.0/AccessBlockerConsolePrototype.dll",
            "args": [],
            "cwd": "${workspaceFolder}/AccessBlockerConsolePrototype",
            // Weitere Informationen zum Feld 'console' finden Sie unter https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
